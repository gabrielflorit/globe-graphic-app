<!DOCTYPE html>
<html lang='en'>
    <head>
        @@include("../components/meta.html", {
            "title": "{{meta.title}}",
            "description": "{{meta.description}}",
            "keywords": "{{meta.keywords}}",
            "url": "{{meta.url}}",
            "img": "{{meta.image_url}}"
        })

        <!-- js to load fonts async -->
        <script src='http://apps.bostonglobe.com/common/js/load-fonts/load-fonts-0.0.2.js' type='text/javascript'></script>
        <!-- fallback for async font loading -->
        <noscript><link href='http://apps.bostonglobe.com/common/font/nojs-fonts.css' rel='stylesheet'></noscript>

        <!-- Globe apps style reset and basic style guide -->
        <link rel='stylesheet' href='http://apps.bostonglobe.com/common/css/base/base-1.0.1.css'>
        <!-- Globe reusable components style -->
        <link rel='stylesheet' href='http://apps.bostonglobe.com/common/css/components/components-0.1.0.css' />
        
        <!-- smoosh -->
        <link rel='stylesheet' href='css/main.css'>
        <!-- endsmoosh -->
        
        <link rel='shortcut icon' href='http://www.bostonglobe.com/rw/SysConfig/WebPortal/BostonGlobe/Framework/images/favicon.ico' />
    </head>


    </head>
    <body>
        @@include("../components/header.html", {"color": "dark" })
        <div id='globe-graphic-container' class='typeset'>
        <p>hey</p>
        </div>

        <!-- js libs -->
        <script src='http://apps.bostonglobe.com/common/js/jquery/jquery-1.11.2.min.js' type='text/javascript'></script>

        <!-- smoosh -->
        <script src='js/main.js' type='text/javascript'></script>
        <!-- endsmoosh -->

        <!-- meter --> 
        <script src='http://www.bostonglobe.com/js/metercheck.js' type='text/javascript'></script>
        <script>
            var methode = {};
            bglobe.freeviewMeter.init({pageId : '{{meta.page_id}}' , registrationWall : 'non-exempt', webType : 'app', sectionPath : 'apps', debug: false });
        </script>

        {{#unless dev}}
            <!-- analytics -->
            @@include("../components/omniture.html", { "title": "{{meta.section_cap}} | {{meta.title}}", "section": "{{meta.section_cap}}" })
            @@include("../components/chartbeat.html", { "section": "{{meta.section_lower}}" })
        {{/unless}}
    </body>
}
</html>