<!DOCTYPE html>
<html lang='en'>
    <head>
        @@include("../components/meta.html", {
            "title": "{{meta.title}}",
            "section": "{{meta.section_cap}}",
            "description": "{{meta.description}}",
            "keywords": "{{meta.keywords}}",
            "url": "{{meta.url}}",
            "img": "{{meta.image_url}}"
        })

        <!-- smoosh -->
        <script src='js/util/async-svg.js' type='text/javascript'></script>
        <script src='js/util/async-css.js' type='text/javascript'></script>
        <script src='js/util/async-font.js' type='text/javascript'></script>
        <link rel='stylesheet' href='css/critical.css' />
        <link rel='stylesheet' href='css/main.css' />
        <!-- endsmoosh -->

        <noscript><link href='http://apps.bostonglobe.com/common/font/nojs-fonts.css' rel='stylesheet' /></noscript>
        <noscript><link rel='stylesheet' href='http://apps.bostonglobe.com/common/css/base/base-1.2.0.css' /></noscript>
        <noscript><link rel='stylesheet' href='http://apps.bostonglobe.com/common/css/components/components-0.2.0.css' /></noscript>

        <link rel='shortcut icon' href='http://www.bostonglobe.com/rw/SysConfig/WebPortal/BostonGlobe/Framework/images/favicon.ico' />
    </head>
    <body>
        @@include("../components/header.html", {"section": "{{meta.section_cap}}", color": "dark" })
        <div id='globe-graphic-container' class='typeset'>






        </div>
        <footer class='g-footer'>
            <ul class='g-footer--credits'>
                {{#each credits}}
                    <li class='g-footer--credits-item'>{{role}} by <span class='g-footer--author'>{{name}}</span></li>
                {{/each}}
            </ul>
            <small class='g-footer--copyright iota'>&copy; <span>2015</span> Boston Globe Media Partners, LLC</small>
        </footer>

        <!-- js libs -->
        <script src='http://apps.bostonglobe.com/common/js/jquery/jquery-1.11.2.min.js' type='text/javascript'></script>

        <!-- smoosh -->
        <script src='js/main.js' type='text/javascript'></script>
        <script src='js/util/ui.js' type='text/javascript'></script>
        <!-- endsmoosh -->

        <!-- meter --> 
        <script src='http://www.bostonglobe.com/js/metercheck.js' type='text/javascript'></script>
        <script>
            var methode = {};
            bglobe.freeviewMeter.init({pageId : '{{meta.page_id}}' , registrationWall : 'non-exempt', webType : 'app', sectionPath : 'apps', debug: false });
        </script>

        {{#unless dev}}
            <!-- analytics -->
            @@include("../components/omniture.html", { "title": "{{meta.section_cap}} | {{meta.title}}", "section": "{{meta.section_cap}}" })
            @@include("../components/chartbeat.html", { "section": "{{meta.section_lower}}" })
        {{/unless}}
    </body>
</html>